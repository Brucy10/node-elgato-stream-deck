{
  "name": "elgato-stream-deck",
  "version": "2.1.1",
  "description": "An npm module for interfacing with the Elgato Stream Deck",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "node-hid": "^0.6.0",
    "sharp": "^0.20.0"
  },
  "devDependencies": {
    "@types/node": "^10.1.3",
    "ava": "^0.23.0",
    "coveralls": "^3.0.0",
    "mockery": "^2.1.0",
    "nyc": "^11.3.0",
    "pureimage": "0.1.3",
    "sinon": "^4.1.2",
    "standard-version": "^4.2.0",
    "stream-buffers": "^3.0.1",
    "tslint": "^5.10.0",
    "tslint-xo": "^0.8.0",
    "typescript": "^2.8.3",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.8"
  },
  "scripts": {
    "prerelease": "npm t",
    "release": "standard-version",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "npm run static",
    "test": "nyc ava",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint:ts": "tslint -c tslint.json --project tsconfig.json \"src/**/*.ts\" \"test/**/*.ts\"",
    "static": "npm run lint:ts",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lange/node-elgato-stream-deck.git"
  },
  "keywords": [
    "elgato",
    "stream",
    "deck",
    "streamdeck",
    "hid",
    "usb",
    "hardware",
    "interface",
    "controller"
  ],
  "author": {
    "name": "Alex Van Camp",
    "email": "email@alexvan.camp",
    "url": "https://alexvan.camp/",
    "twitter": "vancamp"
  },
  "contributors": [
    {
      "name": "Richard Fox",
      "twitter": "ProbablePrime"
    },
    {
      "name": "Chris Hanel",
      "twitter": "chrishanel",
      "email": "chrishanel@gmail.com",
      "url": "http://www.chrishanel.com/"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lange/node-elgato-stream-deck/issues"
  },
  "homepage": "https://github.com/Lange/node-elgato-stream-deck#readme",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=7.6"
  }
}
